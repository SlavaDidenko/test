{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-mixed-spaces-and-tabs": 1,
        "no-extra-semi": 1,
        "no-empty": 1,
        "no-console": 1,
        "indent": 1
    }
}
